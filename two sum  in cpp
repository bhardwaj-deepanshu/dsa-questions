#include <iostream>
#include <vector>
#include <unordered_map>
using namespace std;
class Solution {
public:
    vector<int> twoSum(vector<int>& arr, int tar) {
        unordered_map<int, int> m;
        vector<int> ans;
        for (int i = 0; i < arr.size(); i++) {
            int first=arr[i];
            int sec=tar-first;
            if(m.find(sec)!=m.end()){
                ans.push_back(m[sec]);
                ans.push_back(i);
                break;
            }
            m[first]=i;
        }
        return ans;
    }
};

int main() {
    Solution obj;
    vector<int> nums = {2, 7, 11, 15};
    int target = 9;

    vector<int> result = obj.twoSum(nums, target);

    cout << "Indices: ";
    for (int index : result)
        cout << index << " ";

    return 0;
}
